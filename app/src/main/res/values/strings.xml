	<string name="CategoryCode_ALDR_Description">Sites devoted to the use and sale of drugs and alcohol.</string>
	<string name="CategoryCode_REL_Description">Sites devoted to discussion of religious issues, both supportive and critical, as well as discussion of minority religious groups.</string>
	<string name="CategoryCode_PORN_Description">Hard-core and soft-core pornography.</string>
	<string name="CategoryCode_PROV_Description">Sites which show provocative attire and portray women wearing minimal clothing.</string>
	<string name="CategoryCode_POLR_Description">Content that offers critical political viewpoints (e.g. oppositional political organizations, pro-democracy content, etc.).</string>
	<string name="CategoryCode_HUMR_Description">Content that offers discussions on human rights issues. Includes women\'s rights and rights of minority ethnic groups.</string>
	<string name="CategoryCode_ENV_Description">Sites discussing environmental issues (e.g. pollution, international environmental treaties, deforestation, disasters, etc.).</string>
	<string name="CategoryCode_MILX_Description">Sites promoting terrorism, violent militant or separatist movements.</string>
	<string name="CategoryCode_HATE_Description">Content that disparages particular groups or persons based on race, sex, sexuality or other characteristics.</string>
	<string name="CategoryCode_NEWS_Description">Major news websites (BBC, CNN, etc.) as well as regional news outlets and independent media.</string>
	<string name="CategoryCode_XED_Description">Sites providing information on contraception, abstinence, STDs, healthy sexuality, teen pregnancy, rape prevention, abortion, sexual rights, and sexual health services.</string>
	<string name="CategoryCode_PUBH_Description">Sites providing information on public health issues (e.g. HIV, SARS, bird flu, centers for disease control, World Health Organization, etc.).</string>
	<string name="CategoryCode_GMB_Description">Online gambling sites.</string>
	<string name="CategoryCode_ANON_Description">Sites that provide tools for anonymization, censorship circumvention and encryption.</string>
	<string name="CategoryCode_DATE_Description">Online dating sites.</string>
	<string name="CategoryCode_GRP_Description">Online social networking tools and platforms.</string>
	<string name="CategoryCode_LGBT_Description">Sites hosted by Lesbian, Gay, Bisexual, Transgender, Queer, Intersex (LGBTQI) communities or discussing LGBTQI issues (excluding pornography).</string>
	<string name="CategoryCode_FILE_Description">Sites used to share files, including cloud-based file storage, torrents and P2P file-sharing tools.</string>
	<string name="CategoryCode_HACK_Description">Sites dedicated to computer security, including news and tools.</string>
	<string name="CategoryCode_COMT_Description">Sites and tools for individual and group communications (includes webmail, VoIP, instant messaging, chat and mobile messaging applications).</string>
	<string name="CategoryCode_MMED_Description">Video, audio and photo sharing platforms.</string>
	<string name="CategoryCode_HOST_Description">Web hosting services, blogging and other online publishing platforms.</string>
	<string name="CategoryCode_SRCH_Description">Search engines and portals.</string>
	<string name="CategoryCode_GAME_Description">Online games and gaming platforms (excluding gambling sites).</string>
	<string name="CategoryCode_CULTR_Description">Content relating to entertainment, history, literature, music, film, books, satire and humour.</string>
	<string name="CategoryCode_ECON_Description">Sites discussing general economic development and poverty related topics.</string>
	<string name="CategoryCode_GOVT_Description">Government-run websites, including military sites.</string>
	<string name="CategoryCode_COMM_Description">Sites of commercial services and products.</string>
	<string name="CategoryCode_CTRL_Description">Benign or innocuous content used as a control.</string>
	<string name="CategoryCode_IGO_Description">Sites of intergovernmental organizations, such as the United Nations.</string>
	<string name="CategoryCode_MISC_Description">Sites that haven\'t been categorized yet.</string>
