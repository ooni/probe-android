	<PreferenceScreen
		android:icon="@drawable/privacy"
		android:key="@string/privacy"
		android:title="@string/Settings_Privacy_Label">
		<SwitchPreferenceCompat
			android:defaultValue="true"
			android:key="@string/upload_results"
			android:title="@string/Settings_Sharing_UploadResults"
			app:iconSpaceReserved="false" />
	</PreferenceScreen>
	<PreferenceScreen
		android:icon="@drawable/advanced"
		android:key="@string/advanced"
		android:title="@string/Settings_Advanced_Label">
		<SwitchPreferenceCompat
			android:defaultValue="false"
			android:key="@string/theme_enabled"
			android:title="@string/Settings_DarkMode_Label"
			app:iconSpaceReserved="false" />
		<ListPreference
			android:dialogTitle="@string/Settings_Advanced_LanguageSettings_PopUp"
			android:entries="@array/language_sort_options"
			android:entryValues="@array/language_sort_options_values"
			android:key="@string/language_setting"
			android:summary=""
			android:title="@string/Settings_Advanced_LanguageSettings_Title"
			android:defaultValue="auto"
			app:iconSpaceReserved="false"/>
		<SwitchPreferenceCompat
			android:defaultValue="false"
			android:key="@string/debugLogs"
			android:title="@string/Settings_Advanced_DebugLogs"
			app:iconSpaceReserved="false" />
		<Preference
			android:key="storage_usage"
			android:title="@string/Settings_Storage_Label"
			android:summary=""
			android:widgetLayout="@layout/preference_button"
			app:iconSpaceReserved="false" />
		<SwitchPreferenceCompat
			android:defaultValue="true"
			android:key="@string/warn_vpn_in_use"
			android:title="@string/Settings_WarmVPNInUse_Label"
