			<SwitchPreferenceCompat
				android:defaultValue="true"
				android:key="@string/max_runtime_enabled"
				android:title="@string/Settings_Websites_MaxRuntimeEnabled"
				app:iconSpaceReserved="false" />
			<EditTextPreference
				android:defaultValue="90"
				android:dependency="@string/max_runtime_enabled"
				android:key="@string/max_runtime"
				android:title="@string/Settings_Websites_MaxRuntime"
				app:iconSpaceReserved="false" />
		</PreferenceScreen>
		<PreferenceScreen
			android:icon="@drawable/settings_instant_messaging"
			android:key="@string/instant_messaging"
			android:title="@string/Test_InstantMessaging_Fullname">
